CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_id_patrocinadores_por_torneo` (IN `pid` INT)  NO SQL
SELECT tpatrocinadoresxtorneo.id_torneo, tpatrocinadoresxtorneo.id_patrocinador
FROM tpatrocinadoresxtorneo
WHERE tpatrocinadoresxtorneo.id_torneo = pid$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_patrocinador` ()  NO SQL
SELECT *
FROM tpatrocinador$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_profesores` ()  NO SQL
SELECT * from tbprofesor$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_profesores_por_aproximacion_nombre` (IN `pfiltro` VARCHAR(30) CHARSET utf8)  NO SQL
SELECT tbprofesor.*, tacademia.nombre FROM tbprofesor, tacademia
WHERE tacademia.id_academia = tbprofesor.id_academia AND (tbprofesor.nombre1 like CONCAT('%',pfiltro,'%') or tbprofesor.nombre2 like CONCAT('%',pfiltro,'%') or tbprofesor.apellido1 like CONCAT('%',pfiltro,'%') or tbprofesor.apellido2 like CONCAT('%',pfiltro,'%') or tbprofesor.cedula like CONCAT('%',pfiltro,'%') or tbprofesor.telefono like CONCAT('%',pfiltro,'%') or tbprofesor.correo like CONCAT('%',pfiltro,'%') or tbprofesor.genero like CONCAT('%',pfiltro,'%') or tbprofesor.fecha like CONCAT('%',pfiltro,'%')  or tbprofesor.estado like CONCAT('%',pfiltro,'%') or tacademia.nombre like CONCAT('%',pfiltro,'%'))
ORDER BY tbprofesor.nombre1 ASC$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_torneo` ()  NO SQL
SELECT * FROM ttorneo$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_listar_usuarios` ()  NO SQL
SELECT * FROM tbusuarios$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_modicar_alumno` (IN `pid_academia` INT, IN `pid_profesor` INT, IN `pnombre` VARCHAR(25) CHARSET utf8, IN `papellido1` VARCHAR(25) CHARSET utf8, IN `papellido2` VARCHAR(25) CHARSET utf8, IN `pcorreo` VARCHAR(30) CHARSET utf8, IN `pcedula` VARCHAR(20) CHARSET utf8, IN `ptelefono` VARCHAR(15) CHARSET utf8, IN `pedad` INT, IN `pgenero` VARCHAR(9) CHARSET utf8, IN `pfecha_nacimiento` DATE, IN `ppeso` INT, IN `paltura` INT, IN `pgrado` VARCHAR(8) CHARSET utf8, IN `pcategoria_edad` VARCHAR(10) CHARSET utf8, IN `pcategoria_peso` VARCHAR(20) CHARSET utf8, IN `ptorneos_participados` INT, IN `ptorneos_ganados` INT, IN `pexhibiciones_participadas` INT, IN `pdireccion` TEXT CHARSET utf8, IN `pid` INT)  NO SQL
UPDATE talumno
SET id_academia = pid_academia,
    id_profesor = pid_profesor,
    nombre = pnombre,
    apellido1 = papellido1,
    apellido2 = papellido2,
    correo = pcorreo,
    cedula = pcedula,
    telefono = ptelefono,
    edad = pedad,
    genero = pgenero,
    fecha_nacimiento =  pfecha_nacimiento,
    peso = ppeso,
    altura = paltura,
    grado = pgrado,
    categoria_edad = pcategoria_edad,
    categoria_peso = pcategoria_peso,
    torneos_participados = ptorneos_participados,
    torneos_ganados = ptorneos_ganados,
    exhibiciones_participadas = pexhibiciones_participadas,
    direccion = pdireccion
WHERE id_alumno = pid$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_modicar_patrocinador` (IN `pnombre_patrocinador` VARCHAR(25) CHARSET utf8, IN `pnombre_sociedad_anonima` VARCHAR(25) CHARSET utf8, IN `pcorreo` VARCHAR(30) CHARSET utf8, IN `ptipo_patrocinio` VARCHAR(15) CHARSET utf8, IN `ptelefono` VARCHAR(15) CHARSET utf8, IN `pmonto` INT, IN `pdescripcion_producto` TEXT CHARSET utf8, IN `pid` INT)  NO SQL
UPDATE tpatrocinador
