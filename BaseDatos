CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_filtrar_aproximacion_alumno` (IN `pnombre` VARCHAR(25) CHARSET utf8)  NO SQL
SELECT talumno.*, tacademia.nombre as nombre_academia, tbprofesor.nombre1 as nombre_profesor
FROM talumno, tacademia, tbprofesor
WHERE (tacademia.id_academia = talumno.id_academia) AND (tbprofesor.id_profesor = talumno.id_profesor) AND (talumno.nombre like CONCAT('%',pnombre,'%') or talumno.apellido1 like CONCAT('%',pnombre,'%') or talumno.apellido2 like CONCAT('%',pnombre,'%') or talumno.correo like CONCAT('%',pnombre,'%') or talumno.cedula like CONCAT('%',pnombre,'%') or talumno.telefono like CONCAT('%',pnombre,'%') or talumno.edad like CONCAT('%',pnombre,'%') or talumno.genero like CONCAT('%',pnombre,'%') or talumno.fecha_nacimiento like CONCAT('%',pnombre,'%') or talumno.peso like CONCAT('%',pnombre,'%') or talumno.altura like CONCAT('%',pnombre,'%') or talumno.grado like CONCAT('%',pnombre,'%') or talumno.categoria_edad like CONCAT('%',pnombre,'%') or talumno.categoria_peso like CONCAT('%',pnombre,'%') or talumno.torneos_participados like CONCAT('%',pnombre,'%') or talumno.torneos_ganados like CONCAT('%',pnombre,'%') or talumno.exhibiciones_participadas like CONCAT('%',pnombre,'%') or talumno.direccion like CONCAT('%',pnombre,'%') or tacademia.nombre like CONCAT ('%',pnombre,'%'))$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_filtrar_aproximacion_exhibicion` (IN `pnombre` VARCHAR(25) CHARSET utf8)  NO SQL
SELECT *
FROM texhibicion
WHERE nombre LIKE CONCAT('%',pnombre,'%') OR fecha_inicio LIKE CONCAT('%',pnombre,'%') OR fecha_fin LIKE CONCAT('%',pnombre,'%') OR precio_entrada LIKE CONCAT('%',pnombre,'%') OR invitado LIKE CONCAT ('%',pnombre,'%') OR estado LIKE CONCAT('%',pnombre,'%')$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_filtrar_aproximacion_fogueo` (IN `pnombre` VARCHAR(25))  NO SQL
SELECT *
FROM tfogueo
WHERE (tfogueo.estado = 1) AND (nombre like CONCAT('%',pnombre,'%') or fecha_inicio like CONCAT('%',pnombre,'%') or fecha_fin like CONCAT('%',pnombre,'%') or precio_entrada like CONCAT('%',pnombre,'%'))$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_filtrar_aproximacion_patrocinador` (IN `pnombre_patrocinador` VARCHAR(25) CHARSET utf8)  NO SQL
SELECT *
FROM tpatrocinador
WHERE nombre_patrocinador like CONCAT('%', pnombre_patrocinador,'%') or nombre_sociedad_anonima like CONCAT('%',pnombre_patrocinador,'%') or correo like CONCAT('%',pnombre_patrocinador,'%') or tipo_patrocinio like CONCAT('%',pnombre_patrocinador,'%') or telefono like CONCAT('%',pnombre_patrocinador,'%') or monto like CONCAT('%', pnombre_patrocinador,'%') or descripcion_producto like CONCAT ('%',pnombre_patrocinador,'%')$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_id_profesor_por_correo` (IN `pCorreo` VARCHAR(30) CHARSET utf8)  NO SQL
SELECT id_profesor FROM tbprofesor
WHERE correo = pCorreo$$

