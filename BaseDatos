CREATE DEFINER=`root`@`localhost` PROCEDURE `filtro_aproximacion_academia` (IN `pnombre` VARCHAR(30) CHARSET utf8)  NO SQL
SELECT *
FROM tacademia
WHERE nombre like CONCAT('%',pnombre,'%') or telefono like CONCAT('%',pnombre,'%')  or correo like CONCAT('%',pnombre,'%') or direccion like CONCAT('%',pnombre,'%')$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `filtro_aproximacion_lugares` (IN `pnombre` VARCHAR(25))  NO SQL
SELECT *
FROM tlugar
WHERE nombre_lugar like CONCAT('%',pnombre,'%') or nombre_encargado like CONCAT('%',pnombre,'%') or telefono like CONCAT('%',pnombre,'%') or direccion like CONCAT('%',pnombre,'%') or correo like CONCAT('%',pnombre,'%')$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `filtro_exhibicion` (IN `pnombre` VARCHAR(30))  NO SQL
SELECT *
FROM texhibicion
WHERE (texhibicion.estado=1) AND (nombre LIKE CONCAT('%',pnombre,'%') OR fecha_inicio like CONCAT('%',pnombre,'%') OR fecha_fin LIKE CONCAT('%',pnombre,'%') OR precio_entrada LIKE CONCAT('%',pnombre,'%') OR estado LIKE CONCAT('%',pnombre,'%') OR invitado LIKE CONCAT('%',pnombre,'%'))$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `filtro_torneo` (IN `pnombre` VARCHAR(30))  NO SQL
SELECT *
FROM ttorneo
WHERE (ttorneo.estado = 1) AND(nombre like CONCAT('%',pnombre,'%') or fecha_inicio like CONCAT('%',pnombre,'%') or fecha_fin like CONCAT('%',pnombre,'%') or precio_entrada like CONCAT('%',pnombre,'%') or estado like CONCAT('%',pnombre,'%'))$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `listar_aproximacion_nombre_Org` (IN `pnombre` VARCHAR(25))  NO SQL
SELECT *
FROM torganizacion
WHERE nombre like CONCAT('%',pnombre,'%') or tipo like CONCAT('%',pnombre,'%') or telefono like CONCAT('%',pnombre,'%') or direccion like CONCAT('%',pnombre,'%') or correo like CONCAT('%',pnombre,'%')$$

