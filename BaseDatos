CREATE DEFINER=`root`@`localhost` PROCEDURE `modificar_academia` (IN `pnombre` VARCHAR(30) CHARSET utf8, IN `pcorreo` VARCHAR(30) CHARSET utf8, IN `ptelefono` VARCHAR(15) CHARSET utf8, IN `pdireccion` VARCHAR(40) CHARSET utf8, IN `pid` INT)  NO SQL
UPDATE tacademia
SET nombre = pnombre,
    correo = pcorreo,
    telefono = ptelefono,
    direccion = pdireccion
    
WHERE id_academia = pid$$

