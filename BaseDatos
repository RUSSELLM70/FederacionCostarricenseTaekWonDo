CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_validar_correo_existente` (IN `pCorreo` VARCHAR(30) CHARSET utf8)  NO SQL
SELECT correo
FROM tbusuarios
WHERE correo = pCorreo$$

CREATE DEFINER=`root`@`localhost` PROCEDURE `pa_verificar_usuario` (IN `pCorreo` VARCHAR(30) CHARSET utf8, IN `pContrasenna` VARCHAR(15) CHARSET utf8)  NO SQL
SELECT id_usuario, nombre, correo, estado, id_rol, id_actor From tbusuarios
WHERE correo = pCorreo and contrasenna = pContrasenna$$

